{
    "id": "gtcomm",
    "name": "home",
    "templateFile": "whendy/templates/index.html",
    "elements": [
        {
            "id": "form-login",
            "setPanel": "#panel_02",
            "element": "form",
            "source": "whendy/json/forms/login.json",
            "method": "request{{if(@x>1){'xxxx'}else{''}}}",
            "eparams": {}
        },
        {
            "id": "banner1",
            "setPanel": "#panel_01",
            "element": "fragment",
            "source": "whendy/json/fragments/one.json",
            "name": "{@JSON_PATH}fragments/webcar-banner.json",
            "method": "load",
            "eparams": {}
        },
        {
            "id": "banner2",
            "setPanel": "#panel_03",
            "element": "menu",
            "source": "whendy/json/menus/main.json",
            "method": "load",
            "eparams": {}
        }
    ],
    "className": [
        "home"
    ],
    "acceptedRoles": [
        "** public **"
    ],
    "cssSheets": [
        "sass/reset.css",
        "sass/main.css"
    ],
    "jsModules": [
        "dist/whendy/frontend/wc/Select.js"
    ],
    "modules": [
        {
            "src": "./Html.js",
            "name": "Html",
            "alias": "Html",
            "component": "wh-html"
        },
        {
            "src": "./Menu.js",
            "name": "WHMenu",
            "alias": "WHMenu",
            "component": "wh-menu"
        },
        {
            "src": "./Form.js",
            "name": "WHForm",
            "alias": "WHForm",
            "component": "wh-form"
        },
        {
            "src": "./Map.js",
            "name": "WHMap",
            "alias": "WHMap",
            "component": "wh-map"
        },
        {
            "src": "./GTMap.js",
            "name": "GTMap",
            "alias": "GTMap",
            "component": "gt-map"
        }
    ]
}